{
  "name": "icalendar-syntax",
  "displayName": "iCalendar Syntax",
  "description": "iCalendar syntax highlighting for VS Code",
  "version": "1.0.0",
  "publisher": "SeptWong",
  "icon": "assets/images/logo.png",
  "galleryBanner": {
    "color": "#1e1f22",
    "theme": "dark"
  },
  "main": "./extension.js",
  "activationEvents": [
    "onLanguage:icalendar"
  ],
  "engines": {
    "vscode": "^1.90.0"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/septwong/vscode-iCalendar"
  },
  "bugs": {
    "url": "https://github.com/septwong/vscode-iCalendar/issues"
  },
  "license": "MIT",
  "categories": [
    "Programming Languages",
    "Snippets",
    "Formatters"
  ],
  "keywords": [
    "icalendar",
    "ical",
    "ics",
    "ifb"
  ],
  "contributes": {
    "languages": [
      {
        "id": "icalendar",
        "aliases": [
          "iCalendar",
          "icalendar"
        ],
        "extensions": [
          ".ical",
          ".icalendar",
          ".ics",
          ".ifb"
        ],
        "configuration": "./language-configuration.json"
      }
    ],
    "grammars": [
      {
        "language": "icalendar",
        "scopeName": "source.icalendar",
        "path": "./syntaxes/icalendar.tmLanguage.json"
      }
    ],
    "snippets": [
      {
        "language": "icalendar",
        "path": "snippets/icalendar.json"
      }
    ]
  },
  "dependencies": {
    "ical-generator": "^10.0.0",
    "node-ical": "^0.22.1"
  },
  "devDependencies": {
    "@types/vscode": "^1.90.0"
  }
}
